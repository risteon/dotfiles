[user]
	email = c.rist@posteo.de
	name = Christoph Rist
[core]
	editor = vim
	excludesfile = ~/dotfiles/gitignore_global
[merge]
	tool = kdiff3
[alias]
  lol = log --graph --decorate --pretty=oneline --abrev-commit --all
  whatadded = log --diff-filter=A
[pull]
	ff = only
[credential]
	helper = cache --timeout=28800
